{
  "name": "@viewar/components",
  "version": "1.4.0",
  "description": "ViewAR's reusable react components",
  "main": "./index.js",
  "scripts": {
    "clean": "rimraf ./build && rimraf ./dist",
    "start": "cross-env DEBUG=true webpack-dev-server --open --mode development --env development_core",
    "start:mock": "cross-env DEBUG=true webpack-dev-server --open --mode development --env development_mock",
    "build:webpack": "npm run clean && webpack -p --mode production --env production",
    "build:package": "npm run clean && webpack -p --mode development --env development --config ./package/webpack.config.js",
    "test": "npm run lint && npm run build:webpack && npm run karma",
    "karma": "karma start ./node_modules/@viewar/webpack/karma",
    "lint": "prettier **/*.{css,scss,md,json} --write && eslint --fix \"**/*.*{js,jsx}\" --ignore-pattern \"build/**/*\" --format pretty",
    "prepublishOnly": "build_package.sh"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "files": [
    "index.js",
    "dist/**/*"
  ],
  "directories": {
    "lib": "dist"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "viewar",
    "react",
    "components",
    "shared"
  ],
  "engines": {
    "node": ">=10.13.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "esmodules": true,
            "browsers": [
              "> 1%",
              "last 2 versions",
              "not ie <= 8"
            ]
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-export-default-from",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties"
      ],
      "@babel/plugin-transform-react-constant-elements"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viewar/components.git"
  },
  "author": "Stefan Friedl <sf@viewar.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/viewar/components/issues"
  },
  "homepage": "https://github.com/viewar/components#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "react-hot-loader": "4.12.18",
    "react-router-dom": "^5.1.2",
    "url-loader": "^3.0.0",
    "viewar-api": "^1.1.0",
    "viewar-core": "^11.60.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-transform-react-constant-elements": "^7.7.4",
    "@babel/preset-env": "^7.7.5",
    "@babel/preset-react": "^7.7.4",
    "@viewar/config-eslint": "^1.10.0",
    "@viewar/webpack": "^1.8.2",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "husky": "^3.1.0",
    "karma": "^4.4.1",
    "lodash": "^4.17.15",
    "puppeteer": "^2.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
